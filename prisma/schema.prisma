generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Senders {
  name        String  @id
  email       String  @db.VarChar(200)
  isValidated Boolean @default(false)
  isDefault   Boolean @default(false)

  @@map("senders")
}
